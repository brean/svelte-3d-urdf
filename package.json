{
	"name": "svelte-3d-urdf",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm run prepare && vite dev",
		"build": "npm run prepare && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"dependencies": {
		"@dimforge/rapier3d-compat": "^0.11.2",
		"@smui/button": "^7.0.0-beta.15",
		"@smui/drawer": "^7.0.0-beta.15",
		"@smui/list": "^7.0.0-beta.15",
		"@smui/select": "^7.0.0-beta.15",
		"@smui/top-app-bar": "^7.0.0-beta.15",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.27.4",
		"@theatre/core": "^0.7.0",
		"@theatre/studio": "^0.7.0",
		"@threlte/core": "^6.1.1",
		"@threlte/extras": "^7.5.1",
		"@threlte/preprocess": "^0.0.2",
		"@threlte/rapier": "^1.1.3",
		"@threlte/theatre": "^2.1.3",
		"@types/three": "^0.157.2",
		"monaco-editor": "^0.44.0",
		"smui-theme": "^7.0.0-beta.15",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-sequential-preprocessor": "^1.0.0",
		"three": "^0.157.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0"
	},
	"type": "module"
}
